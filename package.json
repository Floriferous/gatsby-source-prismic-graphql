{
  "private": false,
  "name": "gatsby-source-prismic-graphql",
  "description": "Source data from Prismic with GraphQL",
  "license": "MIT",
  "author": "Birkir Gudjonsson <birkir.gudjonsson@gmail.com>",
  "homepage": "https://github.com/birkir/gatsby-source-prismic-graphql#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birkir/gatsby-source-prismic-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/birkir/gatsby-source-prismic-graphql/issues"
  },
  "version": "3.5.0",
  "main": "index.js",
  "files": [
    "*.js",
    "components",
    "interfaces",
    "types",
    "utils"
  ],
  "scripts": {
    "watch": "tsc -w --emitDeclarationOnly",
    "build": "tsc --emitDeclarationOnly && babel src --out-dir ./ --extensions '.js,.ts,.tsx'",
    "clean": "rimraf {interfaces,types,components,utils,*.js}",
    "prepare": "npm run clean && cross-env NODE_ENV=production npm run build",
    "start": "babel src --out-dir ./ --extensions '.js,.ts,.tsx' --watch",
    "test": "jest"
  },
  "types": "./types/index.d.ts",
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "apollo-link-context": "^1.0.17",
    "gatsby-image": "^2.2.4",
    "gatsby-source-filesystem": "^2.1.2",
    "gatsby-source-graphql": "^2.1.32",
    "lodash.clonedeep": "^4.5.0",
    "path-to-regexp": "^3.0.0",
    "prismic-javascript": "^2.1.1",
    "traverse": "^0.6.6",
    "url-pattern": "^1.0.3"
  },
  "peerDependencies": {
    "gatsby": "^2.13.37",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.3",
    "@babel/plugin-proposal-class-properties": "7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@babel/runtime": "7.3.1",
    "@types/lodash": "4.14.121",
    "@types/node": "^11.15.10",
    "@types/node-fetch": "2.1.6",
    "@types/react": "16.8.4",
    "@types/styled-components": "^4.1.12",
    "@types/traverse": "^0.6.32",
    "babel-preset-gatsby-package": "^0.3.1",
    "cross-env": "^5.2.0",
    "gatsby": "^2.20.22",
    "jest": "^25.3.0",
    "prettier": "^1.16.4",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "graphql",
    "preview",
    "prismic"
  ]
}
